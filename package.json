{
  "name": "nerve",
  "version": "1.0.0-DEV",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "sonar": "sonar-scanner",
    "prebuild": "npm run clean -s",
    "preversion": "git config  user.email \"noreply@deploy.com\" && git config user.name \"buildprocess\"",
    "release:patch": "mkdir -p ./.git && npm -force version patch -m \"Releasing patch version %s \"",
    "release:minor": "mkdir -p ./.git && npm -force version minor -m \"Releasing minor version %s \"",
    "release:major": "mkdir -p ./.git && npm -force version major -m \"Releasing major version %s \"",
    "increment": "git config  user.email \"noreply@deploy.com\" && git config user.name \"buildprocess\" && npm --no-git-tag-version -force version prerelease --preid=SNAPSHOT && git add package.json && git commit -m \"Preparing next snapshot\"  && git push",
    "echo-version": "printf \"##teamcity[setParameter name='env.PROJECT_VERSION' value='%s']\" ${npm_package_version} ",
    "build": "ng build",
    "test": "ng test --code-coverage",
    "test:watch": "ng test --code-coverage --watch",
    "test:ci": "ng test --code-coverage --configuration production",
    "build:ci": "ng build --configuration production",
    "deploy": "ng deploy --base-href=/angular-boilerplate/",
    "lint": "tsc --noEmit && eslint . --ext js,ts,json --quiet --fix",
    "e2e": "ng e2e",
    "clean": "rimraf ./dist",
    "postinstall": "ng build --configuration production",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && ng test --watch=false --browsers=ChromeHeadless",
      "pre-push": "ng build --configuration production"
    }
  },
  "dependencies": {
    "@angular/animations": "^16.2.1",
    "@angular/cdk": "^16.2.1",
    "@angular/common": "^16.2.1",
    "@angular/compiler": "^16.2.1",
    "@angular/core": "^16.2.1",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^16.2.1",
    "@angular/material": "^16.2.1",
    "@angular/material-moment-adapter": "^18.0.6",
    "@angular/platform-browser": "^16.2.1",
    "@angular/platform-browser-dynamic": "^16.2.1",
    "@angular/router": "^16.2.1",
    "@fontsource/material-icons": "^4.5.4",
    "@fontsource/roboto": "^4.5.5",
    "@juggle/resize-observer": "^3.3.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@videogular/ngx-videogular": "^6.0.0",
    "bootstrap": "^5.2.2",
    "core-js": "^3.21.1",
    "echarts": "^5.6.0",
    "file-saver": "^2.0.5",
    "follow-redirects": ">=1.14.8",
    "jquery": "^3.6.0",
    "log4js": ">=6.4.0",
    "marked": ">=4.0.10",
    "material-icons": "^1.10.8",
    "mediaelement": "^4.2.16",
    "moment": "^2.29.4",
    "nanoid": ">=3.3.1",
    "ng-table-virtual-scroll": "^1.4.0",
    "ngx-echarts": "^14.0.0",
    "ngx-markdown": "^19.0.0",
    "ngx-quill": "^26.0.8",
    "node-forge": ">=1.3.0",
    "query-string": "^7.1.1",
    "rxjs": "^7.5.0",
    "svgxuse": "^1.2.6",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "16.1.1",
    "@angular-eslint/eslint-plugin": "16.1.1",
    "@angular-eslint/eslint-plugin-template": "16.1.1",
    "@angular-eslint/schematics": "^16.1.1",
    "@angular-eslint/template-parser": "16.1.1",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^16.2.1",
    "@angular/language-service": "^16.2.1",
    "@types/file-saver": "^2.0.3",
    "@types/jasmine": "~3.8.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.20.55",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "angular-cli-ghpages": "^1.0.0",
    "ansi-regex": ">=5.0.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jasmine-core": "^3.8.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "karma-sonarqube-unit-reporter": "0.0.23",
    "karma-spec-reporter": "0.0.32",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "pretty-quick": "^3.1.3",
    "protractor": "^3.3.0",
    "puppeteer": "^23.11.1",
    "set-value": ">=4.0.1",
    "sonar-scanner": "^3.1.0",
    "stylelint": "^14.6.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "ts-node": "^10.7.0",
    "typescript": "~4.9.4"
  }
}
